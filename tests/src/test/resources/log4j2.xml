<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <!--        <PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %class{0}.%method:%L - %msg%n"/>-->
            <PatternLayout pattern="%d{HH:mm:ss} %-5p %method:%L - %m%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>